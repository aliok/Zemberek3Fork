<?xml version="1.0" encoding="UTF-8" ?>
<suffix-data>

    <suffix-sets>

        <suffix-set id="Possessive">
            P1sg, P2sg, P3sg, P1pl, P2pl, P3pl
        </suffix-set>

        <suffix-set id="PersonNumberSuffixes">
            A1sg, A2sg, A3sg, A1pl, A2pl, A3pl
        </suffix-set>

        <suffix-set id="CaseSuffixes">
            Dat, Loc, Abl, Acc, Gen, Copula
        </suffix-set>

        <suffix-set id="IMEK_ZAMAN">
            IMEK_RIVAYET_MIS
            ,IMEK_SART_SE
            ,IMEK_HIKAYE_DI
            ,IMEK_ZAMAN_KEN
        </suffix-set>

        <suffix-set id="FIIL_ZAMAN">
            FIIL_GECMISZAMAN_DI
            ,FIIL_GECMISZAMAN_MIS
            ,FIIL_GELECEKZAMAN_ECEK
            ,FIIL_GENISZAMAN_IR
            ,FIIL_SIMDIKIZAMAN_IYOR
            ,FIIL_SIMDIKIZAMAN_MEKTE
        </suffix-set>

        <suffix-set id="FIIL_DONUSUM">
            FIIL_DONUSUM_EN
            ,FIIL_DONUSUM_IM
            ,FIIL_DONUSUM_INTI
            ,FIIL_DONUSUM_IS
            ,FIIL_DONUSUM_ME
            ,FIIL_DONUSUM_ECEK
            ,FIIL_DONUSUM_MIS
            ,FIIL_DONUSUM_ILI
        </suffix-set>

        <suffix-set id="FIIL_BILESIK">
            FIIL_SUREKLILIK_EREK
            ,FIIL_SURERLIK_EDUR
            ,FIIL_SURERLIK_EGEL
            ,FIIL_SURERLIK_EGOR
            ,FIIL_SURERLIK_EKAL
            ,FIIL_DEVAMLILIK_DIKCE
            ,FIIL_YAKLASMA_AYAZ
        </suffix-set>

        <suffix-set id="FIIL_EMIR_ISTEK">
            FIIL_EMIR_O_SIN
            ,FIIL_EMIR_ONLAR_SINLER
            ,FIIL_EMIR_SIZ_IN
            ,FIIL_EMIR_SIZRESMI_INIZ
            ,FIIL_ISTEK_E
            ,FIIL_ISTEK_SENE
            ,FIIL_ISTEK_SENIZE
        </suffix-set>

        <suffix-set id="FIIL_KOK">
            FIIL_MASTAR_CE
            ,FIIL_MASTAR_MEK
            ,FIIL_IMSI_IP
            ,FIIL_DONUSUM_ESICE
            ,FIIL_DONUSUM_ESIYE
            ,FIIL_DONUSUM_ESI
            ,FIIL_OLUMSUZLUK_ME
            ,FIIL_OLUMSUZLUK_MEDEN
            ,FIIL_OLUMSUZLUK_MEKSIZIN
            ,FIIL_SART_SE
            ,FIIL_ZAMAN_INCE
            ,FIIL_ZORUNLULUK_MELI
            ,FIIL_YETENEK_EBIL
            ,FIIL_YETERSIZLIK_E
            ,FIIL_EDILGEN_IL
            ,FIIL_ETTIRGEN_TIR
            ,FIIL_OLDURGAN_T
            ,FIIL_TANIMLAMA_ICI
            ,FIIL_BERI_ELI
            ,FIIL_TEZLIK_IVER
            ,FIIL_BELIRTME_DIK
            ,FIIL_BERABERLIK_IS
        </suffix-set>

        <suffix-set id="FIIL_ZAMAN_ARDISIL">
            A1sg
            ,A2sg
            ,A3sg
            ,A1pl
            ,A2pl
            ,A3pl
            ,IMEK_RIVAYET_MIS
            ,IMEK_SART_SE
            ,IMEK_HIKAYE_DI
            ,IMEK_ZAMAN_KEN
            ,FIIL_GIBI_CESINE
        </suffix-set>

    </suffix-sets>


    <!-- ############################  EKLER ##################################### -->

    <!-- uretim kurralarina iliskin aciklama

      uretim kurallari bir ekin hangi kurallara gore nasil olusacagini belirler.
      Ek yazilim tarafindan okundugunda bu kurallara gore olasi tum uretimlar
      tespit edilir.

      kurallar:
      A:son seslinin ince_kalin olmasina gore a ya da e harfi uretilir.
      I:son seslinin durumuna gore i,I,u ya da U uretilir.
      +x: son harf sesli ise x harfi (ya da kural ise ilgili harfler) eklenir.
      >x: son harf sert ise gore x harfinin sert hali kullanilir.
      ~x: sonrasinda gelen harf sesli ise yumusar.
      kucuk harfler: dogrudan uretime eklenir.
      buyuk harfler: turkceye ozel karakterlerin benzerleri. G yumusak g gibi. dogrudan eklenir.
    -->


    <suffixes>

        <suffix id="BOS_EK" gen=""/>

        <!-- isim yalin. -->
        <suffix id="ISIM_KOK" gen="">
            <ardisil-suffixes>
                CaseSuffixes
                ,Possessive
                ,PersonNumberSuffixes
                ,IMEK_ZAMAN
                ,Plural
                ,With
                ,MadeFor
                ,Ness
                ,Without
                ,Inst
                ,Dim
                ,Dim2
                ,Become
                ,Related
                ,Resemblance
                ,Resemblance2
                ,ISIM_ILGI_CI
                ,Quite
                ,By
                ,FIIL_GIBI_CESINE
            </ardisil-suffixes>
        </suffix>

        <!-- fiil KOK -->
        <suffix id="FIIL_KOK" gen="">
            <allowed-suffixes>
                FIIL_ZAMAN
                ,FIIL_DONUSUM
                ,FIIL_BILESIK
                ,FIIL_KOK
                ,FIIL_EMIR_ISTEK
                ,FIIL_EDILGENSESLI_N
            </allowed-suffixes>
        </suffix>

        <!-- isim cogul eki _ler. "gelsin cay_lar.." -->
        <suffix id="Plural" gen="lAr">
            <allowed-suffixes>
                CaseSuffixes
                ,IMEK_ZAMAN
                ,P1sg
                ,P2sg
                ,P3sg
                ,P1pl
                ,P2pl
                ,A1pl
                ,A2pl
                ,By
                ,Inst
            </allowed-suffixes>
        </suffix>

        <!-- isim kucultme _cik.  "Kedi_cik pek SirinmiS".  -->
        <suffix id="Dim" gen=">cI~k">
            <allowed-suffixes>
                CaseSuffixes
                ,Possessive
                ,PersonNumberSuffixes
                ,IMEK_ZAMAN
                ,Plural
                ,Without
                ,MadeFor
                ,Ness
                ,With
                ,Inst
            </allowed-suffixes>
        </suffix>

        <!-- cegiz: "kedi_ceGiz, kitapcaGIz  -->
        <suffix id="Dim2" gen="cAğIz">
            <allowed-suffixes>
                CaseSuffixes
                ,Possessive
                ,PersonNumberSuffixes
                ,IMEK_ZAMAN
                ,Plural
                ,Without
                ,MadeFor
                ,Ness
                ,With
                ,Inst
            </allowed-suffixes>
        </suffix>

        <!-- isim yonelme : "kedi_ye bak"  en: dative-->
        <suffix id="Dat" gen="+yA">
            <exception id="ON_EK" gen="nA">
                ZAMAN_BELIRTME_KI
                ,ISIM_BULUNMA_KI
                ,P3sg
                ,P3pl
            </exception>
            <allowed-suffixes>
                <!-- kisi suffixesi bue ek icin tartismali. karsiyayim oluyor, ama elmayayim + -->
                PersonNumberSuffixes
                ,IMEK_ZAMAN
                ,Copula
            </allowed-suffixes>
        </suffix>

        <!-- isim kalma : "ev_de yemek var."  en:locative -->
        <suffix id="Loc" gen=">dA">
            <exception id="ON_EK" gen="ndA">
                ZAMAN_BELIRTME_KI
                ,ISIM_BULUNMA_KI
                ,P3sg
                ,P3pl
            </exception>
            <allowed-suffixes>
                IMEK_ZAMAN
                ,PersonNumberSuffixes
                ,Copula
                ,ISIM_BULUNMA_KI
            </allowed-suffixes>
        </suffix>

        <!-- isim yonlenme _den : "okul_dan geliyorum"  en:ablative -->
        <suffix id="Abl" gen=">dAn">
            <exception id="ON_EK" gen="ndAn">
                ZAMAN_BELIRTME_KI
                ,ISIM_BULUNMA_KI
                ,P3sg
                ,P3pl
            </exception>
            <allowed-suffixes>
                IMEK_ZAMAN
                ,PersonNumberSuffixes
                ,Copula
            </allowed-suffixes>
        </suffix>

        <!-- isim tanimlama _dir,  "kedi_dir, bu_dur"  -->
        <suffix id="Copula" gen=">dIr">
            <allowed-suffixes>
                <!-- supheli.. her zaman olmuyor-->
                Plural
            </allowed-suffixes>
        </suffix>

        <!-- isim belirtme _i: "kedi_yi gordun mu+"  -->
        <suffix id="Acc" gen="+yI">
            <exception id="ON_EK" gen="nI">
                ZAMAN_BELIRTME_KI
                ,ISIM_BULUNMA_KI
                ,P3sg
                ,P3pl
            </exception>
        </suffix>

        <!-- isim durum lik: "deli_lik etme.."  -->
        <suffix id="Ness" gen="lI~k">
            <allowed-suffixes>
                CaseSuffixes
                ,Possessive
                ,IMEK_ZAMAN
                ,Copula
                ,Plural
                ,Inst
                ,ISIM_ILGI_CI
            </allowed-suffixes>
        </suffix>

        <!-- isim gibi ce: "insan_ca yasamak istiyor."  -->
        <!-- TODO: isim tamlamasi _i eki eklenmeli mi+ "kisacasi" -->
        <suffix id="Quite" gen=">cA">
            <allowed-suffixes>
                CaseSuffixes
                ,PersonNumberSuffixes
                ,Copula
                ,Plural
                ,Gen
            </allowed-suffixes>
        </suffix>

        <!-- isim andirma _imsi: "odunumsu bir yapisi var"  -->
        <suffix id="Resemblance" gen="ImsI">
            <allowed-suffixes>
                CaseSuffixes
                ,PersonNumberSuffixes
                ,IMEK_ZAMAN
                ,Possessive
                ,Copula
                ,Plural
                ,Gen
                ,Ness
                ,With
                ,Without
                ,Dim2
                ,Inst
            </allowed-suffixes>
        </suffix>

        <!-- isim andirma _si: "odunsu bir yapisi var"  -->
        <suffix id="Resemblance2" gen="+msI">
            <allowed-suffixes kopya-ek="Resemblance">
                Become
            </allowed-suffixes>
        </suffix>

        <!-- isim tamlama _in: "kedi_nin kuyrugu"  !! bu ekin sahiplik eki ile karismis olma durumu var -->

        <suffix id="Gen" gen="+nIn">
            <exception id="SU" gen="yun"/>
            <allowed-suffixes>
                IMEK_ZAMAN
                ,ISIM_BULUNMA_KI
            </allowed-suffixes>
        </suffix>

        <!-- isim bulunma _(de)ki: "kedide_ki pire"  -->
        <suffix id="ISIM_BULUNMA_KI" gen="ki">
            <allowed-suffixes>
                CaseSuffixes
                ,IMEK_ZAMAN
                ,Gen
                ,Inst
                ,Plural
            </allowed-suffixes>
        </suffix>

        <!--
             isim tarafindan _ce: "yapilanlar ogrenciler_ce normal karsilandi. "
             isim tamlamasi gelirse oncesinde farkli olusuma neden olur. mahkemesi-nce
        -->
        <suffix id="By" gen=">cA">
            <exception id="ON_EK" gen="ncA">
                P3sg
            </exception>
        </suffix>

        <!-- isim fiil donusumu _le(mek): "deriyi tuz_la_mamiz lazim"  -->
        <suffix id="ISIM_DONUSUM_LE" gen="lA">
            <exception id="SIMDIKI_ZAMAN" gen="l"/>
            <allowed-suffixes kopya-ek="FIIL_KOK"/>
        </suffix>

        <!-- isim fiil donusumu _les(mek): "ahmak_lasmak gerekmez."  -->
        <suffix id="Become" gen="lAş">
            <allowed-suffixes kopya-ek="FIIL_KOK"/>
        </suffix>

        <!-- isim icinde bulunma  _li: "limon_lu kek."  -->
        <suffix id="With" gen="lI">
            <allowed-suffixes>
                CaseSuffixes
                ,IMEK_ZAMAN
                ,PersonNumberSuffixes
                ,Possessive
                ,Plural
                ,Gen
                ,Ness
                ,Dim2
                ,By
                ,Inst
                ,Become
            </allowed-suffixes>
        </suffix>

        <!-- isim "icin yapilmis" _lik : "limon_luk_ta seni bekliyorlar."  -->
        <suffix id="MadeFor" gen="lI~k">
            <allowed-suffixes>
                CaseSuffixes
                ,IMEK_ZAMAN
                ,PersonNumberSuffixes
                ,Possessive
                ,Plural
                ,Gen
                ,Dim2
                ,Ness
                ,By
                ,Inst
                ,Become
                ,With
                ,ISIM_ILGI_CI
                ,Without
            </allowed-suffixes>
        </suffix>

        <!-- isim birliktelik _ile: "domates_le ekmek olsun yeter. Ha, bir de tuz."  -->
        <suffix id="Inst" gen="+ylA">
            <allowed-suffixes>
                IMEK_ZAMAN
                ,PersonNumberSuffixes
                ,Copula
            </allowed-suffixes>

        </suffix>

        <!-- isim sahiplik ben _im : "kedi_m margarita deli etti beni."  -->
        <suffix id="P1sg" gen="Im">
            <exception id="SU" gen="yum"/>
            <allowed-suffixes>
                CaseSuffixes
                ,IMEK_ZAMAN
                ,Inst
                ,Gen
                ,By
                ,A2sg
                ,A2pl
            </allowed-suffixes>
        </suffix>

        <!-- isim sahiplik sen _in : "ev-in guzelmis.."  -->
        <suffix id="P2sg" gen="In">
            <exception id="SU" gen="yun"/>
            <allowed-suffixes>
                CaseSuffixes
                ,IMEK_ZAMAN
                ,Inst
                ,Gen
                ,By
                ,A1sg
                ,A1pl
            </allowed-suffixes>
        </suffix>

        <!-- isim sahiplik biz _imiz : "ev_i guzelmis.."  -->
        <suffix id="P3sg" gen="+sI">
            <exception id="SU" gen="yu"/>
            <allowed-suffixes>
                CaseSuffixes
                ,IMEK_ZAMAN
                ,A1sg
                ,A2sg
                ,A1pl
                ,A2pl
                ,Gen
                ,By
                ,Inst
            </allowed-suffixes>
        </suffix>

        <!-- isim sahiplik biz _imiz : "ev_imiz perisan.."  -->
        <suffix id="P1pl" gen="ImIz">
            <exception id="SU" gen="yumuz"/>
            <allowed-suffixes>
                CaseSuffixes
                ,IMEK_ZAMAN
                ,Inst
                ,Gen
                ,By
                ,A2sg
                ,A2pl
            </allowed-suffixes>
        </suffix>

        <!-- isim sahiplik siz _iniz : "ev_iniz guzelmis.."  -->
        <suffix id="P2pl" gen="InIz">
            <exception id="SU" gen="yunuz"/>
            <allowed-suffixes>
                CaseSuffixes
                ,IMEK_ZAMAN
                ,Inst
                ,Gen
                ,By
                ,A1sg
                ,A1pl
            </allowed-suffixes>
        </suffix>

        <!-- isim sahiplik onlar _lari : "ev_leri guzelmis.."  -->
        <suffix id="P3pl" gen="lArI">
            <allowed-suffixes>
                CaseSuffixes
                ,IMEK_ZAMAN
                ,Inst
                ,Gen
                ,By
            </allowed-suffixes>
        </suffix>

        <!-- isim "iliskili" _sal : "kurum_sal yazilim.."  -->
        <!-- bu ek dile sonradan eklenmistir. -->
        <suffix id="Related" gen="sAl">
            <allowed-suffixes>
                CaseSuffixes
                ,IMEK_ZAMAN
                ,Plural
                ,Inst
                ,Gen
                ,Quite
                ,Ness
                ,Become
                ,ISIM_ILGI_CI
            </allowed-suffixes>
        </suffix>

        <!-- isim yokluk _siz : "balyoz_suz olmayacak.. yikamadik Su duvari." -->
        <suffix id="Without" gen="sIz">
            <allowed-suffixes>
                CaseSuffixes
                ,PersonNumberSuffixes
                ,IMEK_ZAMAN
                ,Possessive
                ,Plural
                ,Ness
                ,Become
                ,Quite
                ,Inst
            </allowed-suffixes>
        </suffix>

        <!-- isim ilgi _ci : "oduncu" -->
        <suffix id="ISIM_ILGI_CI" gen=">cI">
            <allowed-suffixes>
                CaseSuffixes
                ,PersonNumberSuffixes
                ,IMEK_ZAMAN
                ,Possessive
                ,Plural
                ,Ness
                ,Become
                ,Quite
                ,Inst
                ,With
                ,Without
            </allowed-suffixes>
        </suffix>


        <!-- isim KISI ben _im : "ne tuhaf adam_im"  -->
        <!-- imek fiili durumunda:  _m : "kucuk_tu_m ufacikti_m top oynadim aciktim."  -->
        <suffix id="A1sg" gen="+yIm">
            <exception id="ON_EK" gen="m">
                FIIL_OLUMSUZLUK_ME
                ,IMEK_SART_SE
                ,IMEK_HIKAYE_DI
            </exception>
            <allowed-suffixes>
                Copula
            </allowed-suffixes>
        </suffix>


        <!-- isim KISI sen _sin : "ne tuhaf adam_sin"  -->
        <!-- imek fiili durumunda _n: "yahu sen iyi bir adamdi_n. adam_sa_n .. " -->
        <suffix id="A2sg" gen="sIn">
            <exception id="ON_EK" gen="n">
                IMEK_SART_SE
                ,IMEK_HIKAYE_DI
                ,FIIL_SART_SE
                ,FIIL_GECMISZAMAN_DI

            </exception>
            <allowed-suffixes>
                Copula
            </allowed-suffixes>
        </suffix>

        <!-- isim KISI o"  -->
        <suffix id="A3sg" gen=""/>

        <!-- isim KISI biz _iz : "ne tuhaf millet_iz"  -->
        <!-- imek fiili durumunda _k: ""Kurttu_k kopek olduk dedi sinirlice."" -->
        <suffix id="A1pl" gen="+yIz">
            <exception id="ON_EK" gen="k">
                IMEK_SART_SE
                ,IMEK_HIKAYE_DI
                ,FIIL_SART_SE
                ,FIIL_GECMISZAMAN_DI
            </exception>
            <exception id="ON_EK" gen="lIm">
                FIIL_ISTEK_E
            </exception>
            <allowed-suffixes>
                Copula
            </allowed-suffixes>
        </suffix>


        <!-- isim KISI siz _siniz : "ne tuhaf adam_siniz"  -->
        <!-- imek fiili durumunda _niz: "Tembelseniz ben ne yapayim+" -->
        <suffix id="A2pl" gen="sInIz">
            <exception id="ON_EK" gen="nIz">
                IMEK_SART_SE
                ,IMEK_HIKAYE_DI
                ,FIIL_SART_SE
                ,FIIL_GECMISZAMAN_DI
            </exception>
            <allowed-suffixes>
                Copula
            </allowed-suffixes>
        </suffix>

        <!-- isim KISI onlar _lar : "ne tuhaf adam_lar"  -->
        <suffix id="A3pl" gen="lAr">
            <allowed-suffixes>
                IMEK_ZAMAN
                ,Copula
                ,FIIL_GIBI_CESINE
            </allowed-suffixes>
        </suffix>




        <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
        <!-- ~~~~~~~~~~~~~~~~~~~~~~ OZEL ISIM    ~~~~~~~~~~~~~~~~~~~~~~ -->
        <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

        <suffix id="OZEL_KOK" gen="">
            <allowed-suffixes>
                CaseSuffixes
                ,IMEK_ZAMAN
                ,PersonNumberSuffixes
                ,Possessive
                ,Plural
                ,Gen
                ,Dim2
                ,Dim
                ,Ness
                ,By
                ,Inst
                ,Become
                ,With
                ,Without
                ,ISIM_ILGI_CI
                ,Quite
            </allowed-suffixes>
        </suffix>

        <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
        <!-- ~~~~~~~~~~~~~~~~~~~~~~ SAYI suffixesI  ~~~~~~~~~~~~~~~~~~~~~~ -->
        <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
        <!-- sayilar (numeral adjectives) belirtme sifatlari grubuna girer.  -->


        <!-- Ulestirme sayi eki, distributive numeral adjective : bir-er, iki-Ser  -->

        <suffix id="SAYI_ULESTIRME_ER" gen="+şAr">
            <allowed-suffixes kopya-ek="ISIM_KOK"/>
        </suffix>

        <!-- Kesir Sayi eki: "uC-te bir", "altI-da beS" -->
        <suffix id="SAYI_KESIR_DE" gen=">dA">
            <allowed-suffixes kopya-ek="ISIM_KOK"/>
        </suffix>

        <!-- Sira Sayi eki, ordinal numeral adjective : bir-inci, altI-ncI-->
        <suffix id="SAYI_SIRA_INCI" gen="IncI">
            <allowed-suffixes kopya-ek="ISIM_KOK"/>
        </suffix>

        <!-- Topluluk sayi eki : "dOrd-Uz, altI-z" -->
        <suffix id="SAYI_TOPLULUK_IZ" gen="Iz">
            <allowed-suffixes kopya-ek="ISIM_KOK"/>
        </suffix>

        <!--  geometrik kose eki : altI-gen, UC-gen -->
        <suffix id="SAYI_KOSE_GEN" gen="gen">
            <allowed-suffixes kopya-ek="ISIM_KOK"/>
        </suffix>

        <!--  Asil sayi sifati, numeral adjective -->
        <suffix id="SAYI_KOK" gen="">
            <allowed-suffixes kopya-ek="ISIM_KOK">
                SAYI_ULESTIRME_ER
                ,SAYI_KESIR_DE
                ,SAYI_SIRA_INCI
                ,SAYI_TOPLULUK_IZ
                ,SAYI_KOSE_GEN
            </allowed-suffixes>
        </suffix>

        <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
        <!-- ~~~~~~~~~~~~~~~~~~~~~~ FIIL suffixesI  ~~~~~~~~~~~~~~~~~~~~~~ -->
        <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

        <!-- aglamaca yok. -->
        <suffix id="FIIL_MASTAR_CE" gen=">cA">
            <allowed-suffixes kopya-ek="FIIL_DONUSUM_MIS"/>
        </suffix>

        <!-- ... demir pencenin bilegini yakar-casina
             acitmasini hissedince, keske biraz pesrev yapsaydim diye hayiflanmisti... -->
        <suffix id="FIIL_GIBI_CESINE" gen="cAsInA"/>

        <!-- ye-dikce yiyesin geliyor. -->
        <suffix id="FIIL_DEVAMLILIK_DIKCE" gen=">dIkçA"/>

        <!-- gel-en -->
        <suffix id="FIIL_DONUSUM_EN" gen="+yAn">
            <allowed-suffixes>
                CaseSuffixes
                ,Possessive
                ,PersonNumberSuffixes
                ,IMEK_ZAMAN
                ,Plural
                ,Without
                ,MadeFor
                ,Ness
                ,With
                ,Inst
                ,Become
            </allowed-suffixes>
        </suffix>

        <suffix id="FIIL_DONUSUM_ILI" gen="IlI">
            <allowed-suffixes kopya-ek="FIIL_DONUSUM_EN"/>
        </suffix>

        <!-- bu gelisler artik sIkmaya basladi. -->
        <suffix id="FIIL_DONUSUM_IS" gen="+yIş">
            <allowed-suffixes kopya-ek="FIIL_DONUSUM_EN">
                FIIL_GIBI_CESINE
                ,ISIM_ILGI_CI
                ,P3sg
            </allowed-suffixes>
        </suffix>

        <!-- yatik, kalkik . genellestirmek dogru olmayabilir.-->
        <!--        <suffix id="FIIL_DONUSUM_IK" gen="+yI~k">
            <allowed-suffixes kopya-ek="FIIL_DONUSUM_EN"/>
        </suffix>-->

        <suffix id="FIIL_DONUSUM_IM" gen="+nIm">
            <allowed-suffixes kopya-ek="FIIL_DONUSUM_EN">
                ISIM_ILGI_CI
            </allowed-suffixes>
        </suffix>

        <suffix id="FIIL_DONUSUM_INTI" gen="+yIntI">
            <allowed-suffixes kopya-ek="FIIL_DONUSUM_EN"/>
        </suffix>

        <!-- gidecsuffixes hazirlansin. yapacak bir sey yok.  -->
        <suffix id="FIIL_DONUSUM_ECEK" gen="+yAcA~k">
            <allowed-suffixes>
                CaseSuffixes
                ,Possessive
                ,Plural
                ,Gen
                ,Copula
            </allowed-suffixes>
        </suffix>

        <suffix id="FIIL_DONUSUM_MIS" gen="mIş">
            <allowed-suffixes>
                CaseSuffixes
                ,Possessive
                ,Plural
                ,Gen
                ,Copula
            </allowed-suffixes>
        </suffix>

        <suffix id="FIIL_DONUSUM_ESICE" gen="+yAsIcA">
            <allowed-suffixes kopya-ek="FIIL_DONUSUM_EN"/>
        </suffix>

        <suffix id="FIIL_DONUSUM_ESI" gen="+yAsI">
