                ,IMEK_ZAMAN
                ,ISIM_COGUL_LER
                ,ISIM_BULUNMA_LI
                ,ISIM_BULUNMA_LIK
                ,ISIM_DURUM_LIK
                ,ISIM_YOKLUK_SIZ
                ,ISIM_BIRLIKTELIK_LE
                ,ISIM_KUCULTME_CIK
                ,ISIM_KUCULTME_CEGIZ
                ,ISIM_TAMLAMA_I
                ,ISIM_DONUSUM_LES
                ,ISIM_ILISKILI_SEL
                ,ISIM_ANDIRMA_IMSI
                ,ISIM_ANDIRMA_SI
                ,ISIM_ILGI_CI
                ,ISIM_GIBI_CE
                ,ISIM_TARAFINDAN_CE
                ,ZAMAN_BELIRTME_KI
            </ardisil-ekler>
        </ek>

        <ek ad="ZAMAN_BELIRTME_KI" uretim="ki">
            <ozel-durum ad="ZAMAN_KI" uretim="kü"/>
            <ardisil-ekler>
                ISIM_HAL
                ,IMEK_ZAMAN
                ,ISIM_COGUL_LER
            </ardisil-ekler>
        </ek>

        <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
        <!-- ~~~~~~~~~~~~~~~~~~~~~~ ZAMIR EKLERI  ~~~~~~~~~~~~~~~~~~~~~~ -->
        <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

        <ek ad="ZAMIR_SAHIPLIK_IN" uretim="In">
            <ardisil-ekler>
                IMEK_ZAMAN
                ,ISIM_BIRLIKTELIK_LE
                ,ISIM_BULUNMA_KI
                ,ISIM_TANIMLAMA_DIR
                ,ISIM_BELIRTME_I
            </ardisil-ekler>
        </ek>


        <ek ad="ZAMIR_SAHIPLIK_IM" uretim="Im">
            <ardisil-ekler kopya-ek="ZAMIR_SAHIPLIK_IN"/>
        </ek>

        <ek ad="ZAMIR_KOK" uretim="">
            <ardisil-ekler>
                ISIM_HAL
                ,ISIM_SAHIPLIK
                ,ISIM_KISI
                ,IMEK_ZAMAN
                ,ISIM_COGUL_LER
                ,ISIM_BULUNMA_LI
                ,ISIM_DURUM_LIK
                ,ISIM_YOKLUK_SIZ
                ,ISIM_BIRLIKTELIK_LE
                ,ISIM_KUCULTME_CEGIZ
                ,ISIM_TAMLAMA_I
                ,ISIM_TARAFINDAN_CE
                ,ZAMIR_SAHIPLIK_IM
                ,ZAMIR_SAHIPLIK_IM
            </ardisil-ekler>
        </ek>

        <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
        <!-- ~~~~~~~~~~~~~~~~~~~~~~ SORU EKI ~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
        <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

        <ek ad="SORU_KOK" uretim="">
            <ardisil-ekler>
                FIIL_KISI
                ,IMEK_HIKAYE_DI
                ,IMEK_RIVAYET_MIS
                ,ISIM_TANIMLAMA_DIR
            </ardisil-ekler>
        </ek>

        <!-- bu ek normalde kullanilmaz. sadece ceviri gibi islemlerde gerekli olabilir. -->
        <ek ad="FIIL_SORU_MI" uretim="mI">
            <ardisil-ekler>
                FIIL_KISI
                ,FIIL_GECMISZAMAN_DI
                ,FIIL_GECMISZAMAN_MIS
                ,ISIM_TANIMLAMA_DIR
            </ardisil-ekler>
        </ek>

        <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
        <!-- ~~~~~~~~~~~~~~~~~~~~~~ IMEK FIILI ~~~~~~~~~~~~~~~~~~~~~~~~ -->
        <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

        <ek ad="IMEK_KOK" uretim="">
            <ardisil-ekler>
                IMEK_ZAMAN
            </ardisil-ekler>
        </ek>

        <!-- imek "imis" _miS : "bu kelebek tirtil_mis bir zamanlar."  -->
        <ek ad="IMEK_RIVAYET_MIS" uretim="+ymIş">
            <ozellik>HAL</ozellik>
            <ozel-durum ad="ON_EK" uretim="mIş">
                IMEK_KOK
                ,FIIL_KOK
                ,FIIL_OLUMSUZLUK_ME
            </ozel-durum>
            <ardisil-ekler>
                ISIM_KISI
                ,ISIM_HAL
                <!-- dikkat! ardisil IMEK_ZAMAN ekleri kendini yineleme durumu olusturur.-->
                ,IMEK_ZAMAN
            </ardisil-ekler>
        </ek>

        <!-- isim imek "ise" _se : "tukenmez kalem_se kullanma." -->
        <ek ad="IMEK_SART_SE" uretim="+ysA">
            <ozellik>HAL</ozellik>
            <ozel-durum ad="ON_EK" uretim="se">
                IMEK_KOK
            </ozel-durum>
            <ardisil-ekler>
                ISIM_KISI
            </ardisil-ekler>
        </ek>

        <!-- isim imek "idi" _di : "adam aslan_di simdi elin kopegi oldu.." -->
        <ek ad="IMEK_HIKAYE_DI" uretim="+y>dI">
            <ozellik>HAL</ozellik>
            <ozel-durum ad="ON_EK" uretim="di">
                IMEK_KOK
            </ozel-durum>
            <ardisil-ekler>
                ISIM_KISI
            </ardisil-ekler>
        </ek>

        <!-- isim imek "iken" _ken : "ben cocuk_ken buralari karpuz tarlasiydi" -->
        <!-- imek fiilinde "ken" olarak uretilir. -->
        <ek ad="IMEK_ZAMAN_KEN" uretim="+yken">
            <ozel-durum ad="ON_EK" uretim="ken">
                IMEK_KOK
            </ozel-durum>
            <ardisil-ekler>
                ZAMAN_BELIRTME_KI
            </ardisil-ekler>
        </ek>

        <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
        <!-- ~~~~~~~~~~~~~~~~~~~~~~ DIGER KOK EKLER~~~~~~~~~~~~~~~~~~ -->
        <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

        <ek ad="BAGLAC_KOK" uretim=""/>

        <ek ad="EDAT_KOK" uretim="">
            <ardisil-ekler>
                ISIM_KISI
                ,IMEK_ZAMAN
                ,ISIM_TANIMLAMA_DIR
                ,ISIM_TAMLAMA_I
            </ardisil-ekler>
        </ek>

        <ek ad="ZARF_KOK" uretim=""/>

        <ek ad="UNLEM_KOK" uretim=""/>

    </ekler>
